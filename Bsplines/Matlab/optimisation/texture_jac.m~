function [dx, dy] = texture_jac(est, origin, impoint, xgrid, ygrid, xyrange, Phi, d)
    z = evaluate_point(d, x, y, xgrid, ygrid, xyrange, Phi);
    [dx, dy] = spline_df(x, y, xgrid, ygrid, xyrange, Phi, d);
    xterm1 = 2*(est(1) - impoint(1)) + 2*z*dx;
    xterm2 = 2*((origin(1) - impoint(1)) + origin(3)*dx);
    xterm3 = ((a-u)*(x-u) + (b-v)*(y-v) + c*z)^2;
    denom = (a-u)^2 + 
end

